{
  "name": "@jdlfe/minidebug-next",
  "version": "1.0.0",
  "private": false,
  "description": "小程序MiniDebug工具",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "files": [
    "lib",
    "dist",
    "types"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "npm run dev:lib",
    "dev:lib": "tsc --project ./tsconfig.build.json --watch --incremental",
    "build": "npm run build:rollup && npm run build:lib",
    "build:lib": "tsc --project ./tsconfig.build.json",
    "build:rollup": "rollup --config ./config/rollup.config.js"
  },
  "author": "",
  "dependencies": {
    "taro-ui": "^3.0.0-alpha.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@tarojs/cli": "3.0.2",
    "@tarojs/components": "~3.0.2",
    "@tarojs/react": "3.0.2",
    "@tarojs/taro": "3.0.2",
    "@types/react": "^16.0.0",
    "rollup": "^2.21.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "typescript": "^3.7.0"
  }
}
